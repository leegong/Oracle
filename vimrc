set number
set t_Co=111
noremap gs ^
noremap ge $
noremap ; :
inoremap <a-k> <Up>
inoremap <a-j> <Down>
inoremap <a-h> <Left>
inoremap <a-l> <Right>
noremap z i
noremap gc gm
noremap n )
noremap <space> <PageDown>
 set nocompatible 
filetype off 
set rtp+=~/.vim/bundle/Vundle.vim 

filetype plugin indent on 

